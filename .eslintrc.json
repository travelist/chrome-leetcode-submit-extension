{
    "env": {
      
      "es2021": true
    },
    "ignorePatterns": [
      "node_modules/**/*.*"
    ],
    "extends": [
      "airbnb-typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
    ],
    "rules": {
      "max-len": [
        "error",
        {
          "code": 110
        }
      ],
      "@typescript-eslint/semi": [
        "error",
        "never"
      ]
    },
    "settings": {
    }
}
